class Solution {
  constructor(nums) {
    this.nums = nums
  }
  
  reset() {
    return this.nums
  }
  
  shuffle() {
    let result = [...this.nums];
    let n = this.nums.length;
    
    for (let i = 0; i < n; i++) {
      const idx = Math.floor(Math.random() * n);
      [ result[i], result[idx] ] = [ result[idx], result[i] ];
    }
    
    return result;
  }
}